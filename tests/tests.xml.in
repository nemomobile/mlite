<?xml version="1.0" encoding="ISO-8859-1"?>
<testdefinition version="0.1">

    <suite name="mlite">

        <set name="mlite@DASH_QT_VERSION@">

            <description>mlite@DASH_QT_VERSION@ tests</description>

            <case name="ut_mdesktopentry">
                <description>Tests the MDesktopEntry class</description>
                <step>@INSTALL_TESTDIR@/ut_mdesktopentry</step>
            </case>

            <case name="ut_mfiledatastore">
                <description>Tests the MFileDataStore class</description>
                <step>@INSTALL_TESTDIR@/ut_mfiledatastore</step>
            </case>

            <case name="ut_mgconfitem">
                <description>Tests the MGConfItem class</description>
                <step>@INSTALL_TESTDIR@/ut_mgconfitem</step>
            </case>

            <case name="ut_mnotification">
                <description>Tests the MNotification and MNotificationGroup classes</description>
                <step>@INSTALL_TESTDIR@/ut_mnotification</step>
            </case>

            <case name="ut_mremoteaction">
                <description>Tests the MRemoteAction class</description>
                <step>@INSTALL_TESTDIR@/ut_mremoteaction</step>
            </case>

        </set>

    </suite>

</testdefinition>
